{
    "name": "patgod85/phpdoc2rst",
    "type": "symfony-bundle",
    "description": "PHPDoc to RST",
    "homepage": "https://github.com/patgod85/phpdoc2rst",
    "keywords": ["documentation", "phpdoc", "rst"],
    "license" : "MIT",
    "authors": [
        {
            "name":     "Victor",
            "homepage": "https://github.com/patgod85",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.3.2",
        "andrewsville/php-token-reflection": "dev-master",
        "doctrine/annotations": "1.2.*",
        "doctrine/inflector": "1.1.*",
        "jms/serializer": "1.4.*",
        "symfony/config": "3.0.*",
        "symfony/console": "3.0.*",
        "symfony/framework-bundle": "3.0.*",
        "symfony/http-kernel": "3.0.*",
        "symfony/dependency-injection": "3.0.*",
        "symfony/process": "3.0.*",
        "twig/twig": "1.*.*"
    },
    "autoload": {
        "psr-0": { "Patgod85\\Phpdoc2rst": "" }
    },
    "autoload-dev": {
        "psr-0": { "input": "./Tests" }
    },
    "target-dir" : "Patgod85/Phpdoc2rst",
    "require-dev": {
        "mockery/mockery": "0.9.4",
        "phpunit/phpunit": "4.8.*"
    }
}
